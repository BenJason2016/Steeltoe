    <TestSdkVersion>17.0.0</TestSdkVersion>
    <XunitVersion>2.4.1</XunitVersion>
    <XunitStudioVersion>2.4.3</XunitStudioVersion>
    <HealthChecksMySqlVersion>2.2.0</HealthChecksMySqlVersion>
